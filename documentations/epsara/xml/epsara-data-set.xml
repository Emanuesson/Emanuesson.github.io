<refsect2 id="EpsaraDataSetClass" role="struct">
<title>struct EpsaraDataSetClass</title>
<indexterm zone="EpsaraDataSetClass"><primary>EpsaraDataSetClass</primary></indexterm>
<programlisting language="C">struct EpsaraDataSetClass {
  EpsaraDataElementClass parent_class;

  void  (*add)                (EpsaraDataSet         *container,
                               EpsaraDataMeasurement *measurement);
  void  (*remove)             (EpsaraDataSet         *container,
